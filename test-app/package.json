    "ember-template-lint": "^5.11.2",
    "ember-try": "^3.0.0",
    "eslint": "^8.47.0",
    "eslint-plugin-ember": "^11.10.0",
    "loader.js": "^4.7.0",
    "pnpm-sync-dependencies-meta-injected": "^0.0.10",
    "prettier": "^3.0.2",
    "qunit": "^2.19.4",
