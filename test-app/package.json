  "author": "Thomas Gossmann",
  "scripts": {
    "start": "concurrently 'npm:start:*'",
    "start:web-app": "ember serve",
    "start:package": "pnpm run --filter='ember-ability' start",
    "lint": "concurrently -g 'npm:lint:*(!fix)'",
