    "_lint:types": "tsc --noEmit",
    "glint": "glint",
    "_syncPnpm": "pnpm sync-dependencies-meta-injected",
    "test": "ember test -tp 0"
  },
  "dependencies": {
    "ember-ability": "workspace:*"
  },
  "dependenciesMeta": {
    "ember-ability": {
      "injected": true
    }
  },
