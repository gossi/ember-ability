    "@gossi/config-template-lint": "^0.6.0",
    "@tsconfig/ember": "^3.0.0",
    "@types/qunit": "^2.19.6",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.0",
